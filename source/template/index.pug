extends layers/_default


block vars

	-
		page = {
			preheader:'home',
			logo: 'https://city.travel',
			logoSrc: 'https://city.travel/images/ext/img/logo.png',
			imgSrc: 'https://city.travel/images/ext/img/password.png',
			message: ' учётная запись обновлена',
			appTitle: 'Стартовый шаблон',
			description: 'Стартовый шаблон для верстки'
		}


block header				
	+header
block content
	table.content(cellpadding="0" cellspacing="0" border="0" width="100%")
		tr
			td
				center
					.wrapper
						+ie-open({table:640})
						+head
						+yellow-block
						+white-block
						+grey-block
						+promo-offers
						+ie-close
block footer